{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5175 --host",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "npm run test:comprehensive",
    "test:comprehensive": "vite build && node -e \"console.log('Run comprehensive tests in browser at /test-runner.html')\"",
    "test:quick": "node -e \"console.log('Run quick tests in browser console: SeiMoneyTesting.runFullTestSuite()')\"",
    "test:screen": "node -e \"console.log('Test specific screen: SeiMoneyTesting.testScreen(\\\"home\\\")')\"",
    "test:integration": "node -e \"console.log('Test integration: SeiMoneyTesting.testIntegration(\\\"api\\\")')\"",
    "test:setup": "node -e \"console.log('Testing infrastructure ready. Open browser and run tests.')\""
  },
