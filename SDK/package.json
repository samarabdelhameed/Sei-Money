  "author": "Sei Money Team",
  "license": "MIT",
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.32.1",
    "@cosmjs/stargate": "^0.32.1",
    "@cosmjs/proto-signing": "^0.32.1",
    "@cosmjs/amino": "^0.32.1",
    "@cosmjs/crypto": "^0.32.1",
    "@cosmjs/encoding": "^0.32.1",
    "@cosmjs/math": "^0.32.1",
    "@cosmjs/utils": "^0.32.1",
    "cosmjs-types": "^0.9.0",
    "long": "^5.2.1"
